{
  "name": "package-vanilla",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/package-vanilla/package-vanilla.esm.js",
  "exports": {
    ".": {
      "import": "./dist/package-vanilla/package-vanilla.esm.js",
      "require": "./dist/package-vanilla/package-vanilla.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@stencil/react-output-target": "1.0.2",
    "@stencil/vue-output-target": "0.10.7"
  },
  "license": "MIT"
}
